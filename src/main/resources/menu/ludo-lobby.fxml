        style="-fx-background-color: transparent
        -->
        <Button fx:id="startButton" layoutX="290.0" layoutY="400.0" mnemonicParsing="false" onAction="#onStartButtonClick" onMouseEntered="#onStartButtonHover" onMouseExited="#onStartButtonExit" prefHeight="50.0" prefWidth="200.0" style="-fx-background-color: transparent">
